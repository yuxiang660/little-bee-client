{
  "name": "little-bee-client",
  "private": true,
  "description": "A JS frontend client with Gatsby",
  "version": "0.1.0",
  "author": "Little Bee <yuxiang660@gmail.com>",
  "scripts": {
    "build": "gatsby clean && gatsby build",
    "develop": "gatsby clean && gatsby develop",
    "predeploy": "gatsby clean && gatsby build --prefix-paths",
    "start": "npm run develop",
    "serve": "gatsby clean && gatsby build && gatsby serve",
    "clean": "gatsby clean",
    "test": "jest",
    "type-check": "tsc",
    "lint-staged": "lint-staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged && npm run type-check"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": "eslint --ext .js,.jsx,.ts,.tsx",
    "**/*.{js,jsx,tsx,ts,css,md,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@weknow/gatsby-remark-codepen": "^0.1.0",
    "antd": "^3.26.8",
    "autoprefixer": "^9.7.4",
    "classnames": "^2.2.6",
    "gatsby": "^2.18.12",
    "gatsby-image": "^2.2.34",
    "gatsby-plugin-antd": "^2.1.0",
    "gatsby-plugin-manifest": "^2.2.31",
    "gatsby-plugin-offline": "^3.0.27",
    "gatsby-plugin-react-helmet": "^3.1.16",
    "gatsby-plugin-sass": "^2.1.27",
    "gatsby-plugin-sharp": "^2.4.0",
    "gatsby-plugin-typescript": "^2.1.27",
    "gatsby-remark-autolink-headers": "^2.1.23",
    "gatsby-remark-images": "^3.1.42",
    "gatsby-remark-katex": "^3.1.23",
    "gatsby-remark-prismjs": "^3.3.30",
    "gatsby-source-filesystem": "^2.1.40",
    "gatsby-transformer-remark": "^2.6.48",
    "gatsby-transformer-sharp": "^2.3.7",
    "katex": "^0.11.1",
    "lodash": "^4.17.15",
    "lost": "^8.3.1",
    "moment": "^2.24.0",
    "node-sass": "^4.13.1",
    "normalize-scss": "^7.0.1",
    "postcss-pxtorem": "^4.0.1",
    "prismjs": "^1.19.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-disqus-comments": "^1.4.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^9.4.0",
    "@types/classnames": "^2.2.9",
    "@types/jest": "^25.1.2",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.7.0",
    "@types/react-helmet": "^5.0.15",
    "@types/react-test-renderer": "^16.9.2",
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "@typescript-eslint/parser": "^2.19.0",
    "babel-jest": "^25.1.0",
    "babel-preset-gatsby": "^0.2.28",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.18.3",
    "gatsby-plugin-root-import": "^2.0.5",
    "husky": "^4.2.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "react-test-renderer": "^16.12.0",
    "typescript": "^3.8.0-dev.20200206"
  }
}
